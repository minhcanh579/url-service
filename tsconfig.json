{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "baseUrl": "./",
      "paths": {
        "@routes/*": ["src/routes/*"],
        "@services/*": ["src/services/*"],
        "@types/*": ["src/types/*"]
      },
      "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
}